{
	"languageserver": {
    "svls": {
      "command": "svls",
      "filetypes": ["systemverilog", "verilog"]
    }
  },
  "clangd.arguments": [
    "--background-index",
    "--clang-tidy"
  ],
  "clangd.semanticHighlighting": true,
  "cSpellExt.enableDictionaries": ["portuguese-brazilian"],
  "cSpell.language": "en,pt,pt_BR",
  "cSpell.enabledLanguageIds": [
    "css",
    "html",
    "javascriptreact",
    "markdown",
    "plaintext",
    "tex",
    "text"
  ],

  "diagnostic-languageserver.mergeConfig": true,
	"diagnostic-languageserver.filetypes": {
    "c": "cpplint",
    "cpp": "cpplint",
    "css": "stylelint",
    "email": [ "languagetool", "write-good" ],
    "html": "tidy",
    "javascript": "standard",
    "markdown": [ "markdownlint", "languagetool", "write-good" ],
    "python": [ "flake8", "mypy", "pylint" ],
    "sh": "shellcheck",
    "tex": [ "chktex", "textidote" ],
    "vim": "vint"
  },
  "diagnostic-languageserver.formatFiletypes": {
    "css": "stylelint",
    "javascript": "standard",
    "lua": "lua-format",
    "python": ["black", "isort"],
    "sh": "shfmt"
  },
  "diagnostic-languageserver.linters": {
    "chktex": {
      "command": "chktex",
      "debounce": 100,
      "args": ["-q", "--verbosity=3", "--wipeverb=1", "--headererr=1" ],
      "offsetLine": 0,
      "offsetColumn": 0,
      "sourceName": "chktex",
      "formatPattern": [
        "^.*?(\\d+):\\s(.+)$",
        {
          "line": 1,
          "message": 2
        }
      ]
    },
    "lacheck": {
      "command": "lacheck",
      "args": [ "%filename" ],
      "sourceName": "lacheck",
      "formatLines": 1,
      "formatPattern": [
        "^.*?(\\d+):\\s(.+)$",
        {
          "line": 1,
          "message": 2
        }
      ]
    },
    "textidote": {
      "debounce": 100,
      "args": [ "--output", "singleline", "--ignore", "sh:nobreak", "%filename" ],
      "formatLines": 1,
      "formatPattern": [
        "^.*?L(\\d+)C(\\d+).*?\\s(.+?)\"",
        {
          "line": 1,
          "column": 2,
          "endLine": 1,
          "endColumn": 2,
          "message": [3]
        }
      ]
    }
  },

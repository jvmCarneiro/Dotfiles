{
    "parserOptions": {
        "ecmaVersion": 2017,
        "sourceType": "module",
        "ecmaFeatures": {
            "experimentalObjectRestSpread": true,
            "globalReturn": false,
            "impliedStrict": true,
            "jsx": true
        }
    },

    "parser": "esprima",

    "env": {
        "browser": true,
        "node": true,
        "commonjs": true,
        "shared-node-browser": true,
        "es6": true,
        "amd": true,
        "jquery": true,
        "mongo": true
    },

    "plugins": [
        "jsx-a11y",
        "node",
        "react"
    ],

    "extends": "airbnb",

    "rules": {
    }
}
